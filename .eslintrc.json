{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": [2, "double"],
        "comma-style": ["error", "last"]
    }
}
